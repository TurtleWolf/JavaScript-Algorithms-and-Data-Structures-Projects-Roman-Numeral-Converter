function convertToRoman(num) {

while (num > 1){

switch (num) {
  case num > 1000:
    console.log('M');
    break;
  case num > 100:
    console.log('M');
    break;
  case num > 00:
    console.log('M');
    break;
  case num > 1:
    console.log('M');
    break;
  default:
    console.log(num);
}//end of switch
//decrement num!!
}//end of while..


return num;
}

convertToRoman(36);






